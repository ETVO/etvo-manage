{
    "fields": {
        "name": {
            "placeholder": "Full Name",
            "type": "text",
            "required": true
        },
        "email": {
            "placeholder": "Email",
            "type": "email",
            "required": true
        },
        "phone": {
            "placeholder": "Phone",
            "type": "tel",
            "required": true
        },
        "status": {
            "placeholder": "How can we help you?",
            "options": [
                "I want to develop my website",
                "I'm not sure where to start and need guidance",
                "I need a website with complex features",
                "I want to brainstorm ideas and explore possibilities",
                "I need help with website maintenance or updates",
                "I'm interested in e-commerce solutions"
            ],
            "type": "select",
            "required": true
        },
        "message": {
            "placeholder": "Anything to add?",
            "type": "textarea",
            "required": false,
            "attrs": "rows=\"3\""
        }
    },
    "honeypot": "honey",
    "to": "contact@etvo-web.com",
    "subject": "Contact Form Submission",
    "response": {
        "success": "Great! Thank you for your message. ETVO will get in touch with you very soon.",
        "error": "Unfortunately, there was an error. Try again later."
